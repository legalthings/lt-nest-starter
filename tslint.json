{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "eofline": true,
    "quotemark": [
      true,
      "single"
    ],
    "indent": false,
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "arrow-return-shorthand": true,
    "class-name": true,
    "max-classes-per-file": false,
    "cyclomatic-complexity": true,
    "encoding": true,
    "whitespace": true,
    "triple-equals": true,
    "trailing-comma": true,
    "no-trailing-whitespace": true,
    "max-line-length": [
      true,
      120
    ],
    "member-ordering": [
      false
    ],
    "curly": false,
    "interface-name": [
      false
    ],
    "array-type": [
      false
    ],
    "no-empty-interface": false,
    "no-empty": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-namespace": false,
    "variable-name": [
      false
    ],
    "one-line": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ]
  },
  "rulesDirectory": []
}